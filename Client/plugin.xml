<extension
  point="org.eclipse.team.ui.synchronizeParticipants">
  <participant
      persistent="false"
      icon="k.gif"
      class="ivc.sync.org.LocalHistorySubscriber"
      name="Latest From Local History"
      id="sync.LocalSubscriber"/>
</extension>

<extension
  point="org.eclipse.team.ui.synchronizeWizards">
  <wizard
    class="ivc.sync.org.LocalHistorySynchronizeWizard"
    icon="k.gif"
    description="Synchronize resources with their previous contents in the local history"
    name="Synchronize with Latest From Local History"
    id="sync.Wizard"/>
</extension>

